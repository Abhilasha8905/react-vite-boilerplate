{
  "name": "react-vite-boilerplate",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "cross-env BROWSER=chrome vite --host",
    "dev:force": "npm run dev -- --force",
    "build": "vite build",
    "serve": "vite preview --host",
    "start": "npm run dev",
    "lint": "eslint ./src --ext .js,.jsx",
    "preview": "vite preview",
    "prepare": "npm run is-prod || husky install",
    "is-prod": "npx -y check-env-cli NODE_ENV=production"
  },
  "dependencies": {
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@fontsource/roboto": "^4.5.8",
    "@mui/icons-material": "^5.11.0",
    "@mui/material": "^5.11.0",
    "@mui/styled-engine-sc": "^5.11.0",
    "@reduxjs/toolkit": "^1.9.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.5",
    "redux": "^4.2.0",
    "styled-components": "^5.3.6"
  },
  "devDependencies": {
    "@quitsmx/eslint-config": "^0.5.5",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@vitejs/plugin-react": "^3.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.32.0",
    "husky": "^8.0.0",
    "prettier": "github:amarcruz/prettier",
    "vite": "^4.0.0"
  }
}
